{
    "name": "discord-fantasy-music-bot",
    "engines": {
        "node": "14.x"
    },
    "description": "Music bot fo fantasy board gaming",
    "main": "./build/index.js",
    "scripts": {
        "build": "rimraf build && tsc && node postbuild",
        "start": "node build/index.js",
        "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write src && eslint --fix src"
    },
    "keywords": [
        "bot",
        "discord",
        "music",
        "fantasy"
    ],
    "author": "MrFaraday <faradayby@gmail.com>",
    "homepage": "https://github.com/mr-faraday/discord-fantasy-musicbot/#readme",
    "repository": "https://github.com/mr-faraday/discord-fantasy-musicbot.git",
    "license": "MIT",
    "dependencies": {
        "@discordjs/opus": "^0.5.3",
        "axios": "^0.21.1",
        "discord.js": "^12.5.3",
        "express": "^4.17.1",
        "js-video-url-parser": "^0.4.3",
        "lodash.shuffle": "^4.2.0",
        "pg": "^8.6.0",
        "pg-format": "^1.0.4",
        "sodium": "^3.0.2",
        "yt-search": "^2.10.1",
        "ytdl-core": "^4.9.0",
        "ytdl-core-discord": "^1.3.1"
    },
    "devDependencies": {
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/lodash.shuffle": "^4.2.6",
        "@types/node": "^14.14.44",
        "@types/pg": "^7.14.11",
        "@types/pg-format": "^1.0.1",
        "@types/yt-search": "^2.3.2",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "cpy-cli": "^3.1.1",
        "cross-env": "^7.0.3",
        "dotenv": "^8.6.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "ncp": "^2.0.0",
        "prettier": "^2.2.1",
        "prettier-eslint": "^12.0.0",
        "rimraf": "^3.0.2",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.4"
    }
}
