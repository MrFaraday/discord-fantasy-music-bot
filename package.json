{
    "name": "discord-fantasy-music-bot",
    "engines": {
        "node": "^16.x.x"
    },
    "description": "Music bot fo fantasy board gaming",
    "main": "./build/index.js",
    "scripts": {
        "build": "rimraf build && tsc && node postbuild",
        "start": "node build/index.js",
        "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write src && eslint --fix src"
    },
    "keywords": [
        "bot",
        "discord",
        "music",
        "fantasy"
    ],
    "author": "MrFaraday <faradayby@gmail.com>",
    "homepage": "https://github.com/mr-faraday/discord-fantasy-musicbot/#readme",
    "repository": "https://github.com/mr-faraday/discord-fantasy-musicbot.git",
    "license": "MIT",
    "dependencies": {
        "@discordjs/opus": "^0.7.0",
        "@discordjs/rest": "^0.5.0",
        "@discordjs/voice": "^0.10.0",
        "axios": "^0.27.2",
        "discord-api-types": "^0.34.0",
        "discord.js": "^13.10.3",
        "js-video-url-parser": "^0.5.1",
        "lodash.shuffle": "^4.2.0",
        "pg": "^8.8.0",
        "pg-format": "^1.0.4",
        "play-dl": "^1.9.5",
        "sodium": "^3.0.2",
        "uuid": "^8.3.2",
        "yt-search": "^2.10.3",
        "ytdl-core": "^4.11.1"
    },
    "devDependencies": {
        "@types/dotenv": "^8.2.0",
        "@types/lodash.shuffle": "^4.2.7",
        "@types/node": "^18.7.13",
        "@types/pg": "^8.6.5",
        "@types/pg-format": "^1.0.2",
        "@types/uuid": "^8.3.4",
        "@types/yt-search": "^2.3.2",
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
        "cpy-cli": "^4.2.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.1",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "ncp": "^2.0.0",
        "prettier": "^2.7.1",
        "prettier-eslint": "^15.0.1",
        "rimraf": "^3.0.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.8.2"
    }
}
